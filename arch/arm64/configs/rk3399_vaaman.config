# Enable Board Support
CONFIG_BOARD_VICHARAK_VAAMAN=y

# Enable PCA9570 GPIO expander
CONFIG_GPIO_PCA9570=y

# Enable RTL8822CS WiFi
CONFIG_RTL8822CS=y

# Enable Downstream FUSB_302 USB Type-C controller
# CONFIG_TYPEC_FUSB302 is not set
CONFIG_FUSB_302=y

# Build SPI Rockchip driver as module
CONFIG_SPI_ROCKCHIP=m

# Enable RKISP1 support
CONFIG_VIDEO_ROCKCHIP_RKISP1=y
