# Enable board support
CONFIG_BOARD_VICHARAK_AXON=y

# Enable IO Expander support
CONFIG_GPIO_PCA953X=y
CONFIG_GPIO_PCA953X_IRQ=y
CONFIG_SPI_ROCKCHIP=y

# Enable Panthor DRM support
CONFIG_DRM_PANTHOR=y

# Enable support for RTL8852BS wifi module
CONFIG_RTL8852BS=y
CONFIG_RTKBT_UART=y

# Enable serial device bus support
CONFIG_SERIAL_DEV_BUS=y
CONFIG_SERIAL_DEV_CTRL_TTYPORT=y

CONFIG_OF_OVERLAY=y

# Enable overlay fs support
CONFIG_OVERLAY_FS=y
CONFIG_OVERLAY_FS_REDIRECT_DIR=y
CONFIG_OVERLAY_FS_REDIRECT_ALWAYS_FOLLOW=y
# CONFIG_OVERLAY_FS_INDEX is not set
# CONFIG_OVERLAY_FS_XINO_AUTO is not set
# CONFIG_OVERLAY_FS_METACOPY is not set

# DOS/FAT/EXFAT/NT Filesystems
CONFIG_EXFAT_FS=m
CONFIG_EXFAT_DEFAULT_IOCHARSET="utf8"
# end of DOS/FAT/EXFAT/NT Filesystems

CONFIG_USB_AUDIO=m
