#include "rk3588.dtsi"
#include "rk3588-rk806-single.dtsi"
#include <dt-bindings/input/rk-input.h>
#include <dt-bindings/leds/common.h>
#include <dt-bindings/pinctrl/rockchip.h>
#include <dt-bindings/usb/pd.h>

/ {
	compatible = "vicharak,rk3588-axon", "rockchip,rk3588";

	aliases {
		mmc0 = &sdmmc;
		mmc1 = &sdhci;
	};

	es8388_sound: es8388-sound {
		compatible = "rockchip,multicodecs-card";
		hp-con-gpio = <&gpio1 RK_PD5 GPIO_ACTIVE_HIGH>;
		hp-det-gpio = <&gpio1 RK_PC4 GPIO_ACTIVE_LOW>;
		spk-con-gpio = <&gpio1 RK_PC6 GPIO_ACTIVE_HIGH>;
		io-channel-names = "adc-detect";
		io-channels = <&saradc 3>;
		keyup-threshold-microvolt = <1800000>;
		poll-interval = <100>;
		rockchip,audio-routing =
			"Headphone", "LOUT1",
			"Headphone", "ROUT1",
			"Speaker", "LOUT2",
			"Speaker", "ROUT2",
			"Headphone", "Headphone Power",
			"Headphone", "Headphone Power",
			"Speaker", "Speaker Power",
			"Speaker", "Speaker Power",
			"LINPUT1", "Main Mic",
			"LINPUT2", "Main Mic",
			"RINPUT1", "Headset Mic",
			"RINPUT2", "Headset Mic";
		rockchip,card-name = "rockchip-es8388";
		rockchip,codec = <&es8388>;
		rockchip,cpu = <&i2s0_8ch>;
		rockchip,format = "i2s";
		rockchip,mclk-fs = <256>;
		pinctrl-names = "default";
		pinctrl-0 = <&hp_det>;
		status = "okay";

		play-pause-key {
			label = "playpause";
			linux,code = <KEY_PLAYPAUSE>;
			press-threshold-microvolt = <2000>;
		};
	};

	leds {
		compatible = "gpio-leds";
		status = "okay";
		pinctrl-names = "default";
		pinctrl-0 = <&power_led_gpio &status_led_gpio>;

		power_led: power-led {
			function = LED_FUNCTION_POWER;
			gpios = <&gpio0 RK_PB2 GPIO_ACTIVE_HIGH>;
			default-state = "on";
			retain-state-suspended;
			linux,default-trigger = "default-on";
		};

		status_led: status-led {
			function = LED_FUNCTION_STATUS;
			gpios = <&gpio0 RK_PA0 GPIO_ACTIVE_HIGH>;
			default-state = "on";
			retain-state-suspended;
			linux,default-trigger = "heartbeat";
		};
	};

	vbus5v0_typec0: vbus5v0-typec0-regulator {
		compatible = "regulator-fixed";
		regulator-name = "vbus5v0_typec0";
		regulator-boot-on;
		regulator-always-on;
		enable-active-high;
		gpio = <&pca9554 6 GPIO_ACTIVE_HIGH>;
		vin-supply = <&vcc1_5v0>;
	};

	vbus5v0_typec1: vbus5v0-typec1-regulator {
		compatible = "regulator-fixed";
		regulator-name = "vbus5v0_typec1";
		regulator-boot-on;
		regulator-always-on;
		enable-active-high;
		gpio = <&pca9554 4 GPIO_ACTIVE_HIGH>;
		vin-supply = <&vcc1_5v0>;
	};

	vcc12v_dcin: vcc12v-dcin-regulator {
		compatible = "regulator-fixed";
		regulator-name = "vcc12v_dcin";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <12000000>;
		regulator-max-microvolt = <12000000>;
	};

	vcc1_5v0: vcc5v0_sys: vcc1-5v0-regulator {
		compatible = "regulator-fixed";
		regulator-name = "vcc1_5v0";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		vin-supply = <&vcc12v_dcin>;
	};

	vcc_1v1_nldo_s3: vcc-1v1-nldo-s3-regulator {
		compatible = "regulator-fixed";
		regulator-name = "vcc_1v1_nldo_s3";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <1100000>;
		regulator-max-microvolt = <1100000>;
		vin-supply = <&vcc1_5v0>;
	};

	vcc3v3_io_expander: vcc3v3-io-expander {
		compatible = "regulator-fixed";
		regulator-name = "vcc3v3_io_expander";
		regulator-boot-on;
		regulator-always-on;
		enable-active-high;
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		vin-supply = <&vcc_3v3_s3>;
	};

	vcc3v3_usb20: vcc3v3-usb20-regulator {
		compatible = "regulator-fixed";
		regulator-name = "vcc3v3_usb20";
		regulator-boot-on;
		regulator-always-on;
		enable-active-high;
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		vin-supply = <&vcc_3v3_s0>;
	};
};

